{
  "name": "Entension",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["tabs", "scripting"], 
  "host_permissions": ["https://*/*", "http://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16" : "images/16.png",
      "32" : "images/32.png",
      "48" : "images/48.png",
      "128": "images/128.png"
    }
  }, 
  "icons": {
    "16" : "images/16.png",
    "32" : "images/32.png",
    "48" : "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 3
}
